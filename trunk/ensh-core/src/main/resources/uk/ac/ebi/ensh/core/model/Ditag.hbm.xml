<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.compomics.ensh.core.model.impl.DitagImpl" table="ditag" entity-name="Ditag">

        <id name="id" type="integer" unsaved-value="null" column="ditag_id">
            <generator class="native"/>
        </id>

        <set name="genomeLocations" table="ditag_feature">
            <key column="ditag_id"/>
            <one-to-many entity-name="DitagFeature" />
        </set>
        <property name="name"/>
        <property name="type"/>
        <property name="tagCount"  column="tag_count"/>
        <property name="sequence"/>

    </class>

</hibernate-mapping>