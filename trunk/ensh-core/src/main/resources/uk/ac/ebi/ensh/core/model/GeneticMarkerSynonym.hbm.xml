<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.compomics.ensh.core.model.impl.GeneticMarkerSynonymImpl" table="marker_synonym" entity-name="GeneticMarkerSynonym">

        <id name="id" type="integer" unsaved-value="null" column="marker_synonym_id">
            <generator class="native"/>
        </id>

        <many-to-one name="marker" column="marker_id" entity-name="GeneticMarker" />
        <property name="source"/>
        <property name="name"/>

    </class>

</hibernate-mapping>