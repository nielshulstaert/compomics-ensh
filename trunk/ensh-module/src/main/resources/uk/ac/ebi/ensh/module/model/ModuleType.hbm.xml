<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.compomics.ensh.module.model.impl.ModuleTypeImpl" table="type" entity-name="ModuleType">

        <id name="id" type="integer" unsaved-value="null" column="type_id">
            <generator class="native"/>
        </id>

        <property name="name" not-null="true"/>
        <property name="description"/>

    </class>

</hibernate-mapping>