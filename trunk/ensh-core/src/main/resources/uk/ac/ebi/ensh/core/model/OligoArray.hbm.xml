<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.compomics.ensh.core.model.impl.OligoArrayImpl" table="oligo_array" entity-name="OligoArray">

        <id name="id" type="integer" unsaved-value="null" column="oligo_array_id">
            <generator class="native"/>
        </id>

        <many-to-one name="parentArray" entity-name="OligoArray" column="parent_array_id"/>
        <property name="probesetSize" column="probe_setsize"/>
        <property name="name"/>
        <property name="type"/>
        

    </class>

</hibernate-mapping>